{
  "version": 1,
  "paths": {
    "/save": {
      "name": "/save",
      "lambdaFunction": "floomSaveHandler",
      "permissions": {
        "setting": "private",
        "auth": [
          "create",
          "read",
          "update",
          "delete"
        ]
      }
    },
    "/product": {
      "name": "/product",
      "lambdaFunction": "floomProductHandler",
      "permissions": {
        "setting": "private",
        "auth": [
          "create",
          "read",
          "update",
          "delete"
        ]
      }
    },
    "/user": {
      "name": "/user",
      "lambdaFunction": "floomUserHandler",
      "permissions": {
        "setting": "private",
        "auth": [
          "create",
          "read",
          "update",
          "delete"
        ]
      }
    },
    "/public": {
      "name": "/public",
      "lambdaFunction": "floomPublicHandler",
      "permissions": {
        "setting": "open"
      }
    },
    "/report": {
      "name": "/report",
      "lambdaFunction": "floomReportHandler",
      "permissions": {
        "setting": "private",
        "auth": [
          "create",
          "read",
          "update",
          "delete"
        ]
      }
    }
  }
}