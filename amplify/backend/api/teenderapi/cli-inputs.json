{
  "version": 1,
  "paths": {
    "/user": {
      "name": "/user",
      "lambdaFunction": "teenderUserHandler",
      "permissions": {
        "setting": "private",
        "auth": [
          "create",
          "read",
          "update",
          "delete"
        ]
      }
    },
    "/product": {
      "name": "/product",
      "lambdaFunction": "teenderProductHandler",
      "permissions": {
        "setting": "protected",
        "auth": [
          "read"
        ],
        "guest": [
          "read"
        ]
      }
    },
    "/save": {
      "name": "/save",
      "lambdaFunction": "teenderSaveHandler",
      "permissions": {
        "setting": "private",
        "auth": [
          "create",
          "read",
          "update",
          "delete"
        ]
      }
    }
  }
}